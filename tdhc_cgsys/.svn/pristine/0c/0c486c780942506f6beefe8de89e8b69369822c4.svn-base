	<select id="M1S11QCG_H001_M1S1"
		parameterType="code_fb.request.H002request" resultMap="M1S1ResultMap">
		select
		TP_CGCONTRACTMT.C_CHECKWAY,
		TP_CGCONTRACTMT.C_TRANSWAY,
		TP_CGCONTRACTMT.C_CLUDECOM,
		TP_CGCONTRACTMT.C_CLUDETIME,
		TP_CGCONTRACTMT.C_CONLINE,
