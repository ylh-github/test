	<select id="C0011QCG_H001_C001"
		parameterType="code_fb.entity.CG_H001_C001" resultMap="C001ResultMap">
		select
		TP_CGAUTHORITY.C_SUBITEMDES
		,
		TP_CGAUTHORITY.C_SUBITEMID
		,
		TP_CGAUTHORITY.C_ITEMDES
		,
		TP_CGAUTHORITY.C_ENABLED

		from TP_CGAUTHORITY
		WHERE 1=1
		AND TP_CGAUTHORITY.C_SUBITEMDES IS NOT NULL
		AND
		TP_CGAUTHORITY.C_SUBITEMID IS NOT NULL
		<if test="cItemdes != null">
			AND TP_CGAUTHORITY.C_ITEMDES like '' ||
			#{cItemdes,jdbcType=VARCHAR} ||
			''
		</if>
		<if test="cEnabled != null">
			AND TP_CGAUTHORITY.C_ENABLED like '' ||
			#{cEnabled,jdbcType=VARCHAR} ||
			''
		</if>
	</select>
