		TP_CGCONTRACTMT.C_DR,
		TP_CGCONTRACTMT.C_FORINLAND,
		TP_CGCONTRACTMT.C_FOROUTLAND,
		TP_CGCONTRACTMT.C_GETTIME,
		TP_CGCONTRACTMT.C_ID,
		TP_CGCONTRACTMT.C_MODIFIER,
		TP_CGCONTRACTMT.C_MODIFYTIME,
		TP_CGCONTRACTMT.C_ORMTID,
		TP_CGCONTRACTMT.C_ORSTID,
		TP_CGCONTRACTMT.C_PAYWAY,
		TP_CGCONTRACTMT.C_REMARK,
		TP_CGCONTRACTMT.C_SIGNSTEP,
		TP_CGCONTRACTMT.C_STATE,
		TP_CGCONTRACTMT.C_SUPPLIER,
		TP_CGCONTRACTMT.C_TIMESTAMP,
		TP_CGCONTRACTMT.C_CLUDEADDR,
		TP_CGCONTRACTMT.C_SW01,
		TP_CGCONTRACTMT.C_SW02,
		TP_CGCONTRACTMT.C_SW03,
		TP_CGCONTRACTMT.C_SW05,
		TP_CGCONTRACTMT.C_SW06,
		TP_CGCONTRACTMT.C_SW07,
		TP_CGCONTRACTMT.C_SW08,
		TP_CGCONTRACTMT.C_SW09,
		TP_CGCONTRACTMT.C_SW10,
		TP_CGCONTRACTMT.C_SW11,
		TP_CGCONTRACTMT.C_SW12,
		TP_CGCONTRACTMT.C_SW13,
		TP_CGCONTRACTMT.C_SW14,
		TP_CGCONTRACTMT.C_SW15,
		TP_CGCONTRACTMT.C_SW16,
		TP_CGCONTRACTMT.C_SW17,
		TP_CGCONTRACTMT.C_SW18,
		TP_CGCONTRACTMT.C_SW19,
		TP_CGCONTRACTMT.C_SW20,
		TP_CGCONTRACTMT.C_SW21,
		TP_CGCONTRACTMT.C_SW22,
		TP_CGCONTRACTMT.C_SW23,
		TP_CGCONTRACTMT.C_SW24,
		TP_CGCONTRACTMT.C_SW25,
		TP_CGCONTRACTMT.C_SW26,
		TP_CGCONTRACTMT.C_SW27,
		TP_CGCONTRACTMT.C_SW28,
		TP_CGCONTRACTMT.C_SW28,
		TP_CGCONTRACTMT.C_SW29,
		TP_CGCONTRACTMT.C_SIGNPLACE,
		TP_CGCONTRACTMT.C_SW04,
		TP_CGCONTRACTMT.C_CHACONNUM,
		TP_CGCONTRACTMT.C_SECONNUM
		from TP_CGCONTRACTMT
		WHERE 1=1
		and TP_CGCONTRACTMT.C_SW14 = '0'
		<if test="h002request.cCludecomstr != null and h002request.cCludecomstr != ''">
			AND TP_CGCONTRACTMT.C_CLUDECOM IN ( ${h002request.cCludecomstr})  <!-- 签订公司 -->
		</if>
		<if test="h002request.cCheckwaystr != null and h002request.cCheckwaystr != ''">
			AND TP_CGCONTRACTMT.C_CHECKWAY IN ( ${h002request.cCheckwaystr} )  <!-- 到货情况 -->
		</if>
		<if test="h002request.cSignstepstr != null and h002request.cSignstepstr != ''">
			AND TP_CGCONTRACTMT.C_SIGNSTEP IN ( ${h002request.cSignstepstr} )  <!-- 付款情况 -->
		</if>
		<if test="h002request.cTranswaystr != null and h002request.cTranswaystr != ''">
			AND TP_CGCONTRACTMT.C_TRANSWAY IN ( ${h002request.cTranswaystr} )  <!-- 发票情况 -->
		</if>
		<if test="h002request.cConnum != null">
			AND TP_CGCONTRACTMT.C_CONNUM like UPPER('%' || trim(#{h002request.cConnum,jdbcType=VARCHAR}) || '%') <!-- 合同号 -->
		</if>
		<if test="h002request.startTime != null">
			AND TP_CGCONTRACTMT.C_CREATETIME &gt;= #{h002request.startTime,jdbcType=TIMESTAMP}  <!-- 开始时间 -->
		</if>
		<if test="h002request.endTime != null">
			AND TP_CGCONTRACTMT.C_CREATETIME &lt;= #{h002request.endTime,jdbcType=TIMESTAMP}  <!-- 结束时间 -->
		</if>
		<if test="h002request.cState != null">
			AND TP_CGCONTRACTMT.C_STATE like '%' || trim(#{h002request.cState,jdbcType=VARCHAR}) || '%'  <!-- 状态 -->
		</if>
		<if test="h002request.cSupplier != null">
			AND TP_CGCONTRACTMT.C_SUPPLIER like '%' || trim(#{h002request.cSupplier,jdbcType=VARCHAR}) || '%'  <!-- 供应商 -->
		</if>
		<if test=" h002request.cSw10 != null and h002request.cSw10.length != 0 ">
		AND TP_CGCONTRACTMT.C_SW10 IN   <!-- 采购员 -->
		   <foreach collection="h002request.cSw10" item="item" index="index" open="(" separator="," close=")" >
			 #{item}  
		   </foreach>
		</if>
		<if test="h002request.cCreater != null and h002request.cCreater !=''">
			AND TP_CGCONTRACTMT.C_CREATER LIKE  CONCAT('%', CONCAT(TRIM(#{h002request.cCreater,jdbcType=VARCHAR}), '%'))  <!-- 文员 -->
		</if>
		<if test="h002request.cSw28 != null and h002request.cSw28 != ''">
			AND TP_CGCONTRACTMT.C_SW28 LIKE  CONCAT('%', CONCAT(TRIM(#{h002request.cSw28,jdbcType=VARCHAR}), '%')) 
		</if>
	</select>
	
