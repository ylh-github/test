 /** 
	 * S1S21修改按钮
	 *  
	 * */ 
	@RequestMapping(value="/S1S21U",method=RequestMethod.POST) 
	@ResponseBody 
	@RequireUser
	public ResponseObject<EmptyTag, EmptyData> S1S21U(@RequestParamUser User user,@RequestBody RequestObject<EmptyTag,CG_Q001_Request_S1S2> requestObject){
		
		List<CG_Q001_S1S2> list = requestObject.getData().getCgQ001S1s2();
		for(CG_Q001_S1S2 s1s2 : list) {
			s1s2.setcModifier(user.getUserName());
			s1s2.setcModifytime(new Date());
			s1s2.setcRemark(requestObject.getData().getCgQ001S1s2().get(0).getcRemark());
		}
		return cgQ001Service.S1S21U(requestObject.getData(),requestObject.getSession());
	}
