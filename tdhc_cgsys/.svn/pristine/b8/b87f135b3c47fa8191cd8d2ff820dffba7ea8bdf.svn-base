package cn.tdhc.quality.util1;

/**
 * 查询字符串中是否包含字母
 * @author 刘慧南
 *
 */
public class IfIncludeStrUtils {

	public static boolean getStr(String str) {
		boolean flag = false;
		for (int i = 0; i < str.length(); i++) {
			if(Character.isLetter(str.charAt(i))) {
				flag = true;
			}
		}
		return flag;
	}
}
