  <select id="selectMaterials" parameterType="java.lang.String" resultType="code_fb_cg.response.DeCgMaterials">
    select 
    T.C_SW03,T.C_SW05,T.C_SW06,T.C_SW07,T.C_SW04,S.C_CONNUM,M.C_OUTCONNUM,
    T.C_SW08,S.C_SW08 C_GOODSNAME,S.C_SPEC,T.C_SW02,T.C_REMARK,T.C_SW09,M.C_SW10,
    T.C_MUSTNEED,M.C_SUPPLIER,M.C_DR,T.C_GOODSNAME goodsname,T.C_SPEC spec,
    M.C_PAYWAY, M.C_CLUDETIME, S.C_BEFOREARRTIME,S.C_REMARK remark,
    S.C_ARRGOODSTIME, M.C_CHECKWAY, S.C_GOODSNUM, S.C_STATE,S.C_GROUDTOTALNUM,
    S.C_UNIT,S.C_SW06 ship,S.C_SW12,S.C_SW13,S.C_SW14,S.C_SW15
    from TP_CGCONTRACTST S
    LEFT JOIN TP_CGORDERST T ON S.C_SW02 = T.C_ID
    LEFT JOIN TP_CGCONTRACTMT M ON M.C_ID = S.C_MTID
    where 1=1 
    AND S.C_DR = 0
    AND M.C_SW14 = 0
    AND T.C_DR = 0
    <if test="cId != null">
  	AND	S.C_ID = #{cId,jdbcType=VARCHAR}
    </if>
    </select>
    <select id="selectMaterials2" parameterType="java.lang.String" resultType="code_fb_cg.response.DeCgMaterials">
    select 
    S.C_CONNUM,M.C_OUTCONNUM,
    S.C_SW08 C_GOODSNAME,S.C_SPEC,M.C_SW10,
    M.C_SUPPLIER,M.C_DR,S.C_GOODSNAME goodsname,S.C_GROUDTOTALNUM,
    M.C_PAYWAY, M.C_CLUDETIME, S.C_BEFOREARRTIME,S.C_REMARK remark,
    S.C_ARRGOODSTIME, M.C_CHECKWAY, S.C_GOODSNUM, S.C_STATE,
    S.C_UNIT,S.C_SW06 ship,S.C_SW12,S.C_SW13,S.C_SW14,S.C_SW15
    from TP_CGCONTRACTST S
    LEFT JOIN TP_CGCONTRACTMT M ON M.C_ID = S.C_MTID
    where 1=1 
    AND S.C_DR = 0
    AND M.C_SW14 = 0
    <if test="cId != null">
  	AND	S.C_ID = #{cId,jdbcType=VARCHAR}
    </if> 
  </select>
