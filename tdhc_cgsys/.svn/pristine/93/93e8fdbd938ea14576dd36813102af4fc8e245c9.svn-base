  <!-- H006M1查询 -->
  <select id="selectForShip" parameterType="java.lang.String" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from TP_CGCONTRACTST
    where 1=1
    AND C_DR = 0
    		   AND  C_SW04 = '0'<!-- 查询没有出船记录的数据 -->
    	  <if test="cConnum != null" >
		       AND  C_CONNUM like UPPER('%' || trim(#{cConnum,jdbcType=VARCHAR}) || '%')
		  </if>
		   <if test="cGoodsname != null" >
		       AND  C_GOODSNAME like '%' || trim(#{cGoodsname,jdbcType=VARCHAR}) || '%'
		  </if>
		   <if test="cSpec != null" >
		       AND  C_SPEC like '%' || trim(#{cSpec,jdbcType=VARCHAR}) || '%'
		  </if>
		   <if test="cSw03 != null" >
		       AND  C_SW03 like '%' || trim(#{cSw03,jdbcType=VARCHAR}) || '%'<!-- 请购单号 -->
		  </if>
		  <if test="startTime != null" >
			 AND  C_CREATETIME &gt;= #{startTime,jdbcType=TIMESTAMP}
	 	  </if>
		  <if test="endTime != null" >
			 AND  C_CREATETIME &lt;= #{endTime,jdbcType=TIMESTAMP}
		  </if>
  </select>
  <select id="S1S21Q" parameterType="java.lang.String" resultMap="BaseResultMap">
    select 
    TP_CGCONTRACTST.C_ID, TP_CGCONTRACTST.C_MTID, TP_CGCONTRACTST.C_GOODSNAME, TP_CGCONTRACTST.C_SPEC,
    TP_CGCONTRACTST.C_UNIT, TP_CGCONTRACTST.C_PRICE, TP_CGCONTRACTST.C_CONLINE, TP_CGCONTRACTST.C_GOODSNUM, 
    TP_CGCONTRACTST.C_GOODSCASE, TP_CGCONTRACTST.C_SUMPRICE,TP_CGCONTRACTST.C_BEFOREARRTIME,TP_CGCONTRACTST.C_ARRGOODSEX,
    TP_CGCONTRACTST.C_ARRGOODSTIME, TP_CGCONTRACTST.C_GROUDTOTALNUM, TP_CGCONTRACTST.C_RESIDUENUM, 
    TP_CGCONTRACTST.C_ARRALLYORN, TP_CGCONTRACTST.C_REMARK, TP_CGCONTRACTST.C_STATE, TP_CGCONTRACTST.C_CHECKYORN,
    TP_CGCONTRACTST.C_QUADEALLINE, TP_CGCONTRACTST.C_ORID, TP_CGCONTRACTST.C_DR, TP_CGCONTRACTST.C_TIMESTAMP, 
    TP_CGCONTRACTST.C_CREATER, TP_CGCONTRACTST.C_CREATETIME, TP_CGCONTRACTST.C_MODIFIER, TP_CGCONTRACTST.C_MODIFYTIME,
    TP_CGCONTRACTST.C_SW01, TP_CGCONTRACTST.C_SW02, TP_CGCONTRACTST.C_SW03, TP_CGCONTRACTST.C_SW04, 
    TP_CGCONTRACTST.C_SW05, TP_CGCONTRACTST.C_SW06, TP_CGCONTRACTST.C_SW07, TP_CGCONTRACTST.C_SW08, TP_CGCONTRACTST.C_SW09,
    TP_CGCONTRACTST.C_SW10, TP_CGCONTRACTST.C_SW11, TP_CGCONTRACTST.C_CONNUM, TP_CGCONTRACTST.C_SW12, TP_CGCONTRACTST.C_SW13,
    TP_CGCONTRACTST.C_SW14,TP_CGCONTRACTST.C_SW15, TP_CGCONTRACTST.C_SW16, TP_CGCONTRACTST.C_SW17,
    TP_CGCONTRACTST.C_SW18, TP_CGCONTRACTST.C_SW19, TP_CGCONTRACTST.C_SW20
    from TP_CGCONTRACTST
    left join TP_CGCONTRACTMT on TP_CGCONTRACTMT.C_ID = TP_CGCONTRACTST.C_MTID
    where 1=1
    AND TP_CGCONTRACTST.C_DR = 0
    AND TP_CGCONTRACTMT.C_SW14 = 0
    	  
		   <if test="cSw03 != null" >
		       AND  TP_CGCONTRACTST.C_SW03 like '%' || trim(#{cSw03,jdbcType=VARCHAR}) || '%'<!-- 请购单号 -->
		  </if>
		   <if test="cSw12 != null and cSw12 != ''" >
		       AND  TP_CGCONTRACTST.C_SW12 = #{cSw12,jdbcType=VARCHAR}
		  </if>
		   <if test="cSw13 != null and cSw13 != ''" >
		       AND  TP_CGCONTRACTST.C_SW13 = #{cSw13,jdbcType=VARCHAR}
		  </if>
		   <if test="cSw14 != null and cSw14 != ''" >
		       AND  TP_CGCONTRACTST.C_SW14 = #{cSw14,jdbcType=VARCHAR}
		  </if>
		  <if test="cConnum != null">
			AND TP_CGCONTRACTST.C_CONNUM like UPPER(UPPER('%' || trim(#{cConnum,jdbcType=VARCHAR}) || '%')) <!-- 合同号 -->
		</if>
		  <if test="cCheckwaystr != null and cCheckwaystr != ''">
			AND TP_CGCONTRACTMT.C_CHECKWAY IN ( ${cCheckwaystr} )  <!-- 到货情况 -->
		</if>
		<if test="startTime != null">
			AND TP_CGCONTRACTST.C_CREATETIME &gt;= #{startTime,jdbcType=TIMESTAMP}  <!-- 开始时间 -->
		</if>
		<if test="endTime != null">
			AND TP_CGCONTRACTST.C_CREATETIME &lt;= #{endTime,jdbcType=TIMESTAMP}  <!-- 结束时间 -->
		</if>
  </select>
