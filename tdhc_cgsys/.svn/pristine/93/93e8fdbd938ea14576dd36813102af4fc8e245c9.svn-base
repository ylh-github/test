    </if>
    <if test="tbGuildRequest.cConnum != null">
    	 AND C_CONNUM  LIKE UPPER(CONCAT('%',CONCAT(#{tbGuildRequest.cConnum,jdbcType=VARCHAR},'%')))
    </if>
    <if test="tbGuildata.name != null">
    	AND C_SW17 = #{tbGuildata.name,jdbcType=VARCHAR}
    </if>
  </select>
