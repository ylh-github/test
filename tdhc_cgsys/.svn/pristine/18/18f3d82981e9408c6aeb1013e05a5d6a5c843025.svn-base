  <select id="selectByMTST" parameterType="java.lang.String" resultMap="BaseResultMap2">
    select 
    m.C_DR,m.C_ID,m.C_CONNUM,m.C_SUPPLIER,m.C_CLUDEADDR,m.C_CLUDECOM,m.C_CONMONEY,m.C_PAYWAY,m.C_CLUDETIME,
    s.C_SW02,s.C_ORID,s.C_ARRALLYORN,s.C_RESIDUENUM,s.C_GROUDTOTALNUM,
    s.C_GOODSNUM,s.C_GOODSNUM,s.C_MTID,s.C_GOODSNAME,s.C_SPEC,s.C_UNIT,s.C_CONLINE,s.C_REMARK
    from TP_CGCONTRACTMT m, TP_CGCONTRACTST s
    WHERE 1 = 1
    AND m.c_connum = s.c_connum
    AND s.C_STATE IN(0,1,3,5)
    AND s.C_DR = 0
    and m.C_SW14 = '0'
    <if test="cConnum != null" >
       AND  m.C_CONNUM like upper('%' || trim(#{cConnum,jdbcType=VARCHAR}) || '%')
  	</if>
    <if test="cSw10 != null" >
       AND  m.C_SW10 like '%' || trim(#{cSw10,jdbcType=VARCHAR}) || '%'
  	</if>
    <if test="cGoodsname != null" >
       AND  s.C_GOODSNAME like '%' || trim(#{cGoodsname,jdbcType=VARCHAR}) || '%'
  	</if>
  	<if test="cRemark != null and cRemark != ''" >
       AND  s.C_REMARK like '%' || trim(#{cRemark,jdbcType=VARCHAR}) || '%'
  	</if>
  </select>
