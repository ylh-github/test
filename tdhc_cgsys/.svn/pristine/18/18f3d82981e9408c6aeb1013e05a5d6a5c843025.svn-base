  <!-- 统计半个月未到货的 -->
  <select id="selectwdh" parameterType="code_fb_cg.entity.TpCgcontractmt" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from TP_CGCONTRACTMT
    where C_DR-15 &lt;= sysdate
    and C_CHECKWAY != 1
    <if test="(startTime != null and endTime != null)">
  		and C_CREATETIME BETWEEN #{startTime}AND #{endTime}
  	</if>
	order by C_DR
  </select>
