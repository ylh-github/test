	</if>
    <if test="name != '' and name != null ">
        AND C_MANOR in (select distinct C_SUBITEMID from TP_CGAUTHORITY where C_SW01 = #{name,jdbcType=VARCHAR} or c_sw02 = #{name,jdbcType=VARCHAR} or c_sw03 = #{name,jdbcType=VARCHAR})
