  <!-- 维护页面每段时间自动刷新 -->
  <select id="selectInfoTime" resultType="code_fb_cg.response.TpcgcontractmttResponse">
  	select 
  	 TP.C_ID, TP.C_CONNUM, TP.C_STATEXXL as cStatexxk, TP.C_BACKSTATE, TP.C_BACKCAUSE,TP.C_SW18
    FROM TP_CGCONTRACTMTT TP 
    WHERE  1=1
    AND TP.C_STATEXXL IN ('xxk02','xxk05','xxk08') 
 	OR  TP.C_BACKSTATE = 'td002'
  </select>
  <!-- 统计某时间段某采购员拟合同中的合同数量 -->
   <select id="searchNhtnum" resultType="code_fb_cg.entity.TpCgorderstInfo">
    select count(1)nhtNum , sum(tt.c_conmoney)nhtMoney from TP_CGCONTRACTMTT tt 
  WHERE 1=1
	<if test="cManor!=null and cManor !=''">
		AND tt.C_SW10  = #{cManor,jdbcType=VARCHAR}
	</if>
     <if test="(startTime != null and endTime != null)">
	 	AND tt.c_cludetime BETWEEN #{startTime} AND #{endTime}
	 </if>
    AND tt.C_STATEXXL != 'xxk06'
  </select>
  <select id="selectAgreement" resultMap="BaseResultMap" parameterType="java.lang.String" >
    select 
    mt.C_CLUDECOM,
    mt.C_SUPPLIER,
    mt.C_CONMONEY,
    mt.C_CONNUM,
    mt.C_CLUDETIME,
    m.C_SW09 C_SW01
    from TP_CGCONTRACTMTT mt
    left join TP_CGCONTRACTMT m on mt.C_ID = m.C_ID 
    where mt.C_ID = #{cId,jdbcType=VARCHAR}
  </select>
  <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.String" >
    select 
    <include refid="Base_Column_List" />
    from TP_CGCONTRACTMTT
    where C_ID = #{cId,jdbcType=VARCHAR}
  </select>
  <select id="tJXxkSJ" parameterType="code_fb_cg.entity.TpCgordermt2" resultType="code_fb.entity.CG_MNHT_M1S1">
  select 
	t.c_connum,
	t.c_sw10,
	t.c_csubmittime,
	tb.c_createtime,
	tb.c_checktime,
	tb.c_dataedp,
	tb.c_indida
	from TP_CGCONTRACTMTT t 
	left join  TB_DOCUMEFORM tb on tb.c_connum = t.c_connum 
	where 1=1
	<if test="(startTime!=null and endTime !=null)">
	 and t.c_createtime BETWEEN #{startTime} AND #{endTime}
	</if>
	 <if test=" cManor != null and cManor.length != 0 ">
    	AND t.c_sw10 in  <!-- 采购员 -->
       <foreach collection="cManor" item="item" index="index" open="(" separator="," close=")" >
       #{item}  
       </foreach>
    </if> 
	and t.c_csubmittime is not null
  </select>
  <select id="selectByOrcId" resultMap="BaseResultMap" parameterType="java.lang.String" >
    select 
    <include refid="Base_Column_List" />
    from TP_CGCONTRACTMTT
    where C_CONLINE = #{cId,jdbcType=VARCHAR}
  </select>
 <!--  根据合同号 -->
   <select id="selectBycConnum" resultMap="BaseResultMap" parameterType="java.lang.String" >
    select 
    <include refid="Base_Column_List" />
    from TP_CGCONTRACTMTT
    where C_CONNUM = #{cConnum,jdbcType=VARCHAR}
  </select>
 <!--  根据cId 查询是否在此合同有变更协议 -->
   <select id="selectByBgcId" resultMap="BaseResultMap" parameterType="java.lang.String" >
    select 
    <include refid="Base_Column_List" />
    from TP_CGCONTRACTMTT
    where C_QID = #{cId,jdbcType=VARCHAR}
  </select>
<!--   查询合同信息 -->
  <select id="selectInfo" resultMap="BaseResultMap" parameterType="code_fb.entity.CG_MNHT_M1S1" >
    select 
    <include refid="Base_Column_List" />
    from TP_CGCONTRACTMTT
   where 1=1 
   <if test="cg_MNHT_M1S1.cConnum !=null">
    AND C_CONNUM = trim(#{cg_MNHT_M1S1.cConnum,jdbcType=VARCHAR})
   </if>
  </select>
  <!-- 查询合同号是否已存在 -->
  <select id="selectByConnum" parameterType="java.lang.String" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from TP_CGCONTRACTMTT
    where C_CONNUM = UPPER(TRIM(#{cConnum,jdbcType=VARCHAR}))
    and C_SW14 = '0'
  </select>
  <select id="selectByMTSTT" parameterType="code_fb_cg.request.ConMtAndSt" resultType="code_fb_cg.request.ConMtAndSt">
    select 
    m.C_DR,m.C_ID,m.C_CONNUM,m.C_SUPPLIER,m.C_CLUDEADDR,m.C_CLUDECOM,m.C_CONMONEY,m.C_PAYWAY,m.C_CLUDETIME,
    m.C_SW03, m.C_SW09, s.C_SW02,s.C_ORID,s.C_ARRALLYORN,s.C_RESIDUENUM,s.C_GROUDTOTALNUM, m.C_SIGNPLACE, 
    s.C_GOODSNUM,s.C_GOODSNUM,s.C_MTID,s.C_GOODSNAME,s.C_SPEC,s.C_UNIT,s.C_CONLINE,s.C_REMARK, s.C_CUSPEC,m.C_STATEXXL
    from TP_CGCONTRACTMTT m, TP_CGCONTRACTSTT s
    WHERE 1 = 1
    AND m.c_connum = s.c_connum
    AND s.C_STATE IN(0,1,3,5)
    AND s.C_DR = 0
    <if test="conMtAndSt.cConnum != null" >
       AND  m.C_CONNUM like upper('%' || trim(#{conMtAndSt.cConnum,jdbcType=VARCHAR}) || '%')
  	</if>
    <if test="conMtAndSt.cSw10 != null" >
       AND  m.C_SW10 like '%' || trim(#{conMtAndSt.cSw10,jdbcType=VARCHAR}) || '%'
  	</if>
    <if test="conMtAndSt.cGoodsname != null" >
       AND  s.C_GOODSNAME like '%' || trim(#{conMtAndSt.cGoodsname,jdbcType=VARCHAR}) || '%'
  	</if>
  	<if test="conMtAndSt.cRemark != null and conMtAndSt.cRemark != ''" >
       AND  s.C_REMARK like '%' || trim(#{conMtAndSt.cRemark,jdbcType=VARCHAR}) || '%'
  	</if>
  </select>
  
